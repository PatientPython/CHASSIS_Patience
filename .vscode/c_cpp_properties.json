{
  "configurations": [
    {
      "name": "windows-gcc-x64",
      "includePath": [
        "${workspaceFolder}/**"
      ],
      "defines": [
        "_DEBUG",
        "UNICODE",
        "_UNICODE",
        "USE_STDPERIPH_DRIVER",
        "STM32F40_41xxx",
        "ARM_MATH_CM4",
        "__CC_ARM",
        "ARM_MATH_MATRIX_CHECK",
        "ARM_MATH_ROUNDING",
        "__TARGET_FPU_VFP",
        "__FPU_PRESENT=1",
        "__FPU_USED=1"
      ],
      "compilerPath": "C:/mingw64/bin/gcc.exe",
      "cStandard": "${default}",
      "cppStandard": "${default}",
      "intelliSenseMode": "windows-gcc-x64"
    }
  ],
  "version": 4
}